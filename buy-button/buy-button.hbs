<a class="buy-button" href="https://gum.co/{{gumroadSlug}}">
  <span class="buy-button__project">{{@root.productName}} Commercial</span>
  <span class="buy-button__title">{{title}}</span>
  <span class="buy-button__license">License</span>
  <span class="buy-button__for">for {{for}}</span>
  <span class="buy-button__price">
    <span class="buy-button__price__dollar">$</span><span class="buy-button__price__value">{{price}}</span>
  </span>
</a>
